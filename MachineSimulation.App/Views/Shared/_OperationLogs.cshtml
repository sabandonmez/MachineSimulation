@model IEnumerable<OperationLogDto>

<table class="table table-striped table-hover ">
    <thead>
        <tr>

            <th>Eylem</th>
            <th>Zaman Damgası</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.OperationName</td>
                <td>@log.Timestamp.ToString("yyyy-MM-dd HH:mm:ss")</td>
            </tr>
        }
    </tbody>
</table>
